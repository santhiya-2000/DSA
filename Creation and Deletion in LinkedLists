    }
}
int main()
{
    int n,val,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        insertAtLast();
    }
    //deleteAtFirst();
    //deleteAtFirst();
    struct SLLnode* t=head;
    while(t!=NULL)
    {
        printf("%d->",t->data);
        t=t->next;
    }
}
