#include<stdio.h>
#include <stdlib.h>
struct SLLnode
{
    int data;
    struct SLLnode* next;
};
struct SLLnode* head=NULL;
struct SLLnode* last=NULL;
struct SLLnode* createnode()
{
    struct SLLnode* nn=(struct SLLnode*)malloc(sizeof(struct SLLnode));
    scanf("%d",&nn->data);
    nn->next=NULL;    
    return nn;
}
void insertAtFirst()
{
    struct SLLnode* temp=createnode();
    if(head==NULL)
    {
        head=temp;
        last=temp;
    }
    else
    {
        temp->next=head;
        head=temp;
    }
}
void insertAtLast()
{
    struct SLLnode* temp=createnode();
    if(head==NULL)
    {
        head=temp;
        last=temp;
    }
    else
    {
        struct SLLnode* k=head;
        while(k->next!=NULL)
        {
            k=k->next;
        }
        k->next=temp;
        //since we have last we can do last->next=temp. But i just tried :)
        last=temp;
    }
}
struct SLLnode* deleteAtFirst()
{
    if(head==NULL)
    {
        printf("empty");
    }
    else
    {
        struct SLLnode* temp=head;
        head=head->next;
        free(temp);
    }
}
struct SLLnode* deleteAtLast()
{
    if(head==NULL)
    {
        printf("empty");
    }
    else
    {
